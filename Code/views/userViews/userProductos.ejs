<!DOCTYPE html>
<html lang="en">
<head>
    <!-- head --> 
    <%- include('../partials/head') %>
    <!-- /head -->
    <title>DH-Clothes</title>
</head>
<body>
    <!-- header --> 
    <%- include('../partials/header') %>
    <!-- /header --> 
    <main>
        <section class="productos">
            <% for(let i=0; i< products.length; i++){ %>
                <div class="articulo">
                    <a href="producto">
                        <img src= <%= products[i].imagen %> alt="producto-prueba">
                        <h4><%= products[i].nombre%></h4>
                        <p>$<%= products[i].precio%></p>
                    </a> 
                </div>
            <% } %>
        </section>
    </main>
    <!-- footer --> 
    <%- include('../partials/footer') %>
    <!-- /footer --> 
</body>
</html>